    </xs:unique>
    <xs:unique msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true" name="UIStatusTypes_Constraint1">
      <xs:selector xpath=".//mstns:UIStatusTypes" />
      <xs:field xpath="mstns:StatusTypeCd" />
    </xs:unique>
  </xs:element>
</xs:schema>